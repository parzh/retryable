{
  "compilerOptions": {
    "module": "CommonJS",
    "lib": [ "ES2015" ],
    "target": "ES5",
    "outDir": "dist",
    "declaration": true,
    "stripInternal": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "inlineSources": true,

    "strict": true,
    "noUnusedLocals": true,
    "forceConsistentCasingInFileNames": true,
  }
}
