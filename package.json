{
  "name": "@parzh/retryable",
  "version": "1.4.1",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "github:parzh/retryable",
  "keywords": [
    "retryable",
    "retryer",
    "repeat",
    "repeatable",
    "retry",
    "retry count",
    "retryCount"
  ],
  "author": "Dima Parzhitsky <parzhitsky@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/parzh/retryable#readme",
  "bugs": {
    "url": "https://github.com/parzh/retryable/issues"
  },
  "scripts": {
    "test": "cross-env NODE_OPTIONS='--unhandled-rejections=strict' jest",
    "lint": "eslint --ext .ts src test",
    "clean": "rimraf dist/*",
    "prebuild": "npm run clean",
    "build": "ncc build src/index.ts --out dist --minify",
    "prepublishOnly": "npm ci && npm run build && npm test",
    "postpublish": "git push --follow-tags"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "verbose": true,
    "rootDir": ".",
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/*.spec.[jt]s"
    ],
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "restoreMocks": true,
    "errorOnDeprecated": true,
    "cacheDirectory": "<rootDir>/.cache/jest"
  },
  "devDependencies": {
    "@types/jest": "25.1.4",
    "@types/node": "13.9.1",
    "@typescript-eslint/eslint-plugin": "2.23.0",
    "@typescript-eslint/parser": "2.23.0",
    "@zeit/ncc": "0.21.1",
    "cross-env": "7.0.2",
    "eslint": "6.8.0",
    "jest": "26.0.1",
    "rimraf": "3.0.2",
    "ts-jest": "26.0.0",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@valuer/main": "1.1.2"
  }
}
